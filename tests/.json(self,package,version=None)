#!/usr/bin/env python
from unittest import main, skip, TestCase
from apipypi import pypi
from assert_is import *


class TestCase(TestCase):
    #@skip("")
    def test(self):
        is_dict(pypi.json(
            "osascript"
        ))

    def test_version(self):
        is_dict(pypi.json(
            "osascript","0.0.1"
        ))


if __name__ == "__main__":
    main()
